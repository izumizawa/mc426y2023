{
  "extends": ["airbnb", "airbnb/hooks"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "quotes": ["error", "double"],
    "import/prefer-default-export": "off",
    "no-console": "off",
    "no-shadow": "off",
    "react/jsx-no-constructed-context-values": "off"
  },
  "env": {
    "browser": true,
    "jest": true
  }
}